extends ../layouts/nav-bar.pug

block content
  
  if errors
    each error in errors  
      .alert.alert-danger=error

  a.btn.btn-success(href="/users/create") Tạo user mới
  ul
    each user in users
      li
        span=user.name
        a(href="/users/"+user.userId+"/update") Sửa
        a(href="/users/"+user.userId+"/delete") Xóa
  

